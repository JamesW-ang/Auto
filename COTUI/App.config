<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <connectionStrings>
        <add name="COTUIDatabase" 
             connectionString="Data Source=.\Data\COTUI.db;Version=3;" 
             providerName="System.Data.SQLite" />
    </connectionStrings>
    <appSettings>
        <!-- 数据库配置 -->
        <add key="LogRetentionDays" value="30" />
        <add key="AutoBackupEnabled" value="true" />
        <add key="BackupIntervalDays" value="7" />
    </appSettings>
</configuration>